{
  "name": "inertiajs-chrome-extension",
  "version": "1.1.0",
  "description": "A browser extension for debugging Inertia.js applications, built with Bun, React, and TypeScript.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zcuric/inertiajs-chrome-extension"
  },
  "homepage": "https://github.com/zcuric/inertiajs-chrome-extension",
  "scripts": {
    "build": "bun run config/build-with-react-compiler.ts",
    "build:original": "bun run config/build.ts",
    "dev": "bun run config/watch.ts",
    "pack": "bun run config/pack.ts",
    "css": "postcss",
    "release": "bun run release.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@biomejs/biome": "^2.1.1",
    "@types/adm-zip": "^0.5.7",
    "@types/bun": "latest",
    "@types/chrome": "^0.0.332",
    "@types/react-dom": "^19.1.6",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "chalk": "^5.4.1",
    "lefthook": "^1.12.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "tailwindcss": "^3.4.17"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@uiw/react-json-view": "^2.0.0-alpha.32",
    "element-ready": "^8.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
